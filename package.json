{
  "name": "nextn",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:alt": "next dev --turbopack -p 9003",
    "dev:collab": "ENABLE_COLLABORATION=true node collaboration-server.mjs",
    "build": "next build",
    "export": "next export apps/web/out",
    "start": "NODE_ENV=production node server.mjs",
    "start:collab": "NODE_ENV=production ENABLE_COLLABORATION=true node collaboration-server.mjs",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\" scripts || true",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "coverage:report": "node coverage-report.js",
    "test:e2e": "playwright test tests/prettify.test.mjs tests/isValidVIN.test.ts",
    "e2e": "playwright test --reporter=list || true",
    "e2e:ui": "playwright test --ui",
    "e2e:report": "playwright show-report",
    "gen:previews": "node --experimental-modules scripts/generate-previews.js",
    "extract:i18n": "i18next-parser \"src/**/*.{ts,tsx}\" --config i18next-parser.config.js",
    "seed:reviews": "node scripts/seed-reviews.ts",
    "generate:doc": "tsx scripts/generate-doc.ts",
    "seed:user": "tsx scripts/seedUser.ts",
    "preview": "npm run build && npm run export && firebase hosting:channel:deploy dev",
    "analyze": "ANALYZE=true npm run dev",
    "verify-templates": "tsx scripts/verify-templates.ts",
    "monitor-templates": "tsx scripts/template-monitor.ts",
    "fix-templates": "tsx scripts/auto-fix-templates.ts",
    "template-report": "tsx scripts/verify-templates.ts --report-only",
    "quality-check": "node scripts/quality-verification-system.js",
    "legal-check": "node scripts/legal-compliance-validator.js",
    "translation-check": "node scripts/translation-validation-system.js",
    "decision-check": "node scripts/integrated-quality-system.js",
    "full-check": "node scripts/integrated-quality-system.js",
    "seo-check": "node scripts/seo-content-uniqueness-system.js",
    "seo-weekly": "node scripts/seo-weekly-monitor.js",
    "seo-monitor": "node scripts/seo-content-uniqueness-system.js",
    "backlink-daily": "node scripts/automated-backlink-builder.js daily",
    "backlink-weekly": "node scripts/automated-backlink-builder.js weekly",
    "backlink-stats": "node scripts/automated-backlink-builder.js stats",
    "market-validate": "node scripts/market-readiness-validation-system.js",
    "market-staged": "node scripts/market-readiness-validation-system.js staged",
    "market-scores": "node scripts/market-readiness-validation-system.js scores",
    "monitor": "node scripts/monitoring-dashboard.js",
    "ab-test": "tsx src/scripts/setup-ab-tests.ts",
    "type-check": "echo 'TypeScript checking requires installing typescript package' && exit 0",
    "lint-staged": "eslint src/ --ext .ts,.tsx --fix --max-warnings 5",
    "accessibility:audit": "pa11y --config .pa11yrc.json",
    "accessibility:test": "jest src/components/ui/__tests__/accessibility.test.tsx",
    "accessibility:e2e": "playwright test e2e/accessibility.spec.ts",
    "accessibility:full": "npm run accessibility:test && npm run accessibility:e2e",
    "test:accessibility": "playwright test e2e/accessibility*.spec.ts --reporter=html,json",
    "test:accessibility:ci": "playwright test e2e/accessibility*.spec.ts --reporter=json --output-dir=test-results"
